{{ ansible_managed | comment }}

# allowed values: prod | dev | test | qa
box_type={{ box_info_type }}
force_color_prompt=yes

# default, if not set WHITE/U 
BOX_COLOR_default='\[\e[1;37m\]'
BOX_LETTER_default='UNDEF'

# production, RED/P
BOX_COLOR_prod='\[\e[1;31m\]'
BOX_LETTER_prod='PROD'

# testing, YELLOW/T
BOX_COLOR_test='\[\e[1;33m\]'
BOX_LETTER_test='TEST'

# development, GREEN/D
BOX_COLOR_dev='\[\e[1;32m\]'
BOX_LETTER_dev='DEV'

# development, GREEN/D
BOX_COLOR_qa='\[\e[1;32m\]'
BOX_LETTER_qa='QA'
